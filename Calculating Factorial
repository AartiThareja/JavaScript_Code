class Factorial
{
    number;
    constructor(number)
    {
        this.number = number;
    }
    
    factorial()
    {
        let n= this.number;
        let fact = 1;
        while (n > 0)
        {
           fact = fact * n;
            n--;
        }
        console.log("Factorial is " + fact);
    }
}

const facto = new Factorial(5);
facto.factorial();
